{
    "env": {
        "browser": false,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "ignorePatterns": [
        "temp.js",
        "**/*.test.js"
    ],
    "rules": {
        "linebreak-style": 0,
        "react/jsx-filename-extension": "off",
        "import/prefer-default-export": "off",
        "default-param-last": "off",
        "react/function-component-definition": "off",
        "comma-dangle": "off",
        "react/button-has-type": "off",
        "no-plusplus": "off",
        "no-shadow": "off",
        "import/no-extraneous-dependencies": [
            "error",
            {
                "devDependencies": false,
                "optionalDependencies": false,
                "peerDependencies": false
            }
        ],
        "import/no-cycle": [
            "error",
            {
                "maxDepth": 10,
                "ignoreExternal": true
            }
        ]
    },
    "globals": {
        "ethers": "readonly"
    }
}